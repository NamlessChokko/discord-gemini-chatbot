    "main": "dist/index.js",
    "scripts": {
        "build": "rm -rf dist && tsc",
        "start": "node dist/deployCommands.js && node dist/index.js",
        "dev": "nodemon -e ts --watch `./config.json` 'src/**/*.ts' --exec './scripts/run-dev.sh'",
        "lint": "eslint . --ext .ts",
        "format": "prettier --write ."
    },
    "keywords": [],
    "author": "NamlessChokko",
    "license": "MIT",
    "description": "A discord bot that use the Gemini api (kinda chatbot)",
