    "dependencies": {
        "@google/genai": "^0.9.0",
        "@google/generative-ai": "^0.24.0",
        "axios": "^1.8.4",
        "discord.js": "^14.18.0",
        "dotenv": "^16.5.0",
        "highlight.js": "^11.11.1",
        "mime-types": "^3.0.1"
    },
    "devDependencies": {
        "@eslint/js": "^9.25.1",
        "@swc/cli": "^0.7.8",
        "@swc/core": "^1.12.11",
        "@types/highlight.js": "^9.12.4",
        "@types/mime-types": "^3.0.1",
        "@types/node": "^24.0.10",
        "eslint": "^9.25.1",
        "eslint-config-prettier": "^10.1.2",
        "eslint-plugin-prettier": "^5.2.6",
        "globals": "^16.0.0",
        "nodemon": "^3.1.10",
        "prettier": "^3.5.3",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "tsx": "^4.20.3",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.35.1"
    }
