{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "importAssertions": true
    },
    "target": "es2022",
    "preserveAllComments": true,
    "transform": {
      "react": {
        "runtime": "automatic"
      }
    }
  },
  "module": {
    "type": "es6",
    "strict": true,
    "strictMode": true,
    "lazy": false,
    "noInterop": false
  }
}
